<Window x:Class="TestGame.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:SkiaGame.Wpf;assembly=SkiaGame.Wpf"
        xmlns:testGame="clr-namespace:TestGame;assembly=TestGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <wpf:SkiaGameForm Engine="{Binding Engine, Source={x:Static testGame:Game.GameInstance}}" Focusable="True"/>
    </Grid>
</Window>
